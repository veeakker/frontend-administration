{{#if this.kindHasModel}}
  <LinkTo @route={{this.route}} @model={{@model}} ...attributes>
    {{#if (has-block)}}
      {{yield}}
    {{else}}
      {{this.label}}
    {{/if}}
  </LinkTo>
{{else}}
  <LinkTo @route={{this.route}} ...attributes>
    {{#if (has-block)}}
      {{yield}}
    {{else}}
      {{this.label}}
    {{/if}}
  </LinkTo>
{{/if}}